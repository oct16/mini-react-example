@import './variables.styl';

html, body {
    margin: 0 !important;
    font-family: $font-family;
    font-size: $font-size;
    color: $color-mazarine;
    position: relative;
}

table, tbody, tr, td {
    margin: 0;
    padding: 0;
    border-collapse: collapse;
}

ul, li {
    margin: 0;
    padding: 0;
    list-style: none;
}

a {
    text-decoration: none;
    color: #666;
}
